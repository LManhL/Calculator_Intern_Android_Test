<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#004F81" />
            <stroke
                android:width="1dp"
                android:color="#B8B7B7"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#B8B7B7"/>
        </shape>
    </item>
</selector>